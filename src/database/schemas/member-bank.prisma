enum MemberBankTier {
    TIER_0
    TIER_1
    TIER_2
    TIER_3
    TIER_4
    TIER_5
    TIER_6
    TIER_7
    TIER_8
}

model MemberBank {
    userId  String
    guildId String

    member Member @relation(fields: [userId, guildId], references: [userId, guildId], onDelete: Cascade)

    tier  MemberBankTier @default(TIER_0)
    funds Int            @default(0)

    @@id([userId, guildId])
    @@map("member_bank")
}
