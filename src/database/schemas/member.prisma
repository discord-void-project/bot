model Member {
    userId String
    user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)

    guildId String
    guild   Guild  @relation(fields: [guildId], references: [id], onDelete: Cascade)

    activityXp  Int @default(0)
    guildPoints Int @default(0)

    messageCount      Int @default(0)
    voiceTotalMinutes Int @default(0)

    lastDailyAt DateTime?
    lastWorkAt  DateTime?
    lastRobAt   DateTime?

    vault MemberVault?

    @@id([userId, guildId])
}
