model Shop {
    guildId String
    guild   Guild  @relation(fields: [guildId], references: [id], onDelete: Cascade)

    currencyName String
    currency     GuildCurrency @relation(fields: [guildId, currencyName], references: [guildId, name], onDelete: Cascade)

    isOpen    Boolean @default(true)
    name      String
    iconUrl   String?
    bannerUrl String?

    createdAt DateTime @default(now())

    items ShopItem[]

    @@id([guildId, name])
    @@map("shop")
}
