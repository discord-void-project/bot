enum BlacklistStatus {
    PENDING
    ACCEPTED
}

model Blacklist {
    userId String @id
    user   User   @relation("User", fields: [userId], references: [id], onDelete: Cascade)

    modId String
    mod   User   @relation("Mod", fields: [modId], references: [id], onDelete: Cascade)

    status BlacklistStatus @default(PENDING)
    reason      String?

    guilds BlacklistGuild[]

    blacklistAt DateTime @default(now())
}
