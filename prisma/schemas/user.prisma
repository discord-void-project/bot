model User {
    id    String @id
    flags Int    @default(0)

    warner Warn[]   @relation("Mod")
    warned Warn[]   @relation("User")
    guilds Member[]

    blacklister Blacklist[] @relation("Mod")
    blacklisted Blacklist[] @relation("User")

    tagAssignedAt DateTime?
    createdAt     DateTime  @default(now())
}
